			"item": [
				{
					"name": "Authentication",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/x-www-form-urlencoded"
							}
						],
						"body": {
							"mode": "urlencoded",
							"urlencoded": [
								{
									"key": "j_username",
									"value": "{{j_username}}",
									"type": "text"
								},
								{
									"key": "j_password",
									"value": "{{j_password}}",
									"type": "text"
								}
							]
						},
						"url": {
							"raw": "https://{{vmanage}}:{{port}}/j_security_check",
							"protocol": "https",
							"host": [
								"{{vmanage}}"
							],
							"port": "{{port}}",
							"path": [
								"j_security_check"
							]
						}
					},
					"response": []
				}
			]
